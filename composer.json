{
    "name": "guillaumetissier/qrcode",
    "version": "0.1.0",
    "description": "Qr Code Generator for numeric, alphanumeric and byte data",
    "license": "MIT",
    "authors": [
        {
            "name": "Guillaume Tissier",
            "email": "guillaume.tissier@yahoo.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Guillaumetissier\\QrCode\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Guillaumetissier\\QrCode\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "ext-gd": "*",
        "ext-ctype": "*",
        "tecnickcom/tcpdf": "^6.8",
        "symfony/console": "^6.4",
        "monolog/monolog": "^3.8",
        "guillaumetissier/galois-fields": "^1.0",
        "guillaumetissier/bit-string": "^1.0",
        "guillaumetissier/maths": "^0.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests",
        "stan": "phpstan analyse src tests --level=max",
        "cs-check": "phpcs src tests --standard=PSR12",
        "cs-fix": "phpcbf src tests --standard=PSR12"
    },
    "post-install-cmd": [
        "cp -n scripts/pre-commit .git/hooks/pre-commit || true",
        "chmod +x .git/hooks/pre-commit"
    ]
}
